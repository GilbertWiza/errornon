    "@depay/web3-tokens": "^7.2.0",
    "@depay/web3-wallets": "^6.1.2",
    "decimal.js": "^10.3.1",
    "react-rangeslider": "^2.2.0"
  },
  "peerDependencies": {
    "ethers": "^5.4.6",
