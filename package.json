{
  "name": "errornon",
  "moduleName": "DePayWidgets",
  "version": "1.0.0",
  "description": "Web3 Payments with any token. DePay simplifies and improves Web3 Payments with the power of DeFi. Accept any token with on-the-fly conversion.",
  "main": "./dist/cjs/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GilbertWiza/errornon.git"
  },
  "keywords": [
    "web3",
    "quae",
    "quisquam",
    "aut",
    "est",
    "ea",
    "cryptocurrencies"
  ],
  "author": "JenniferUllrich",
  "license": "MIT",
  "scripts": {
    "build": "npx rollup --config rollup.module.config.js && npx rollup --config rollup.bundle.config.js",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "format": "prettier src --write",
    "prepublishOnly": "yarn build",
    "dev": "npx rollup -c rollup.dev.config.js -w",
    "test:cypress": "yarn cypress run",
    "test:cypress:debug": "yarn cypress run --headed --no-exit",
    "test:jest": "npx jest --no-cache",
    "test": "yarn test:jest && yarn test:cypress"
  },
